void showHSVLogo() {

  for(int i = 0; i < NUM_LEDS; i++) {
    long value = pgm_read_dword(&(syLogoHSV[i]));
    int hue = (value & 0xFFF0000) >> 16;
    int saturation = (value & 0x000FF00) >> 8;
    int brightness = (value & 0x00000FF);

    CHSV hsvValue( hue, saturation, brightness);
    // hsv2rgb_spectrum( hsvValue, rgbConverted1);
    leds[i] = hsvValue;
  }

  FastLED.show();
  delay(2000);

}

void showRGBLogo() {

  for(int i = 0; i < NUM_LEDS; i++) {
    long value = pgm_read_dword(&(syLogoRGB[i]));

    leds[i] = value;
  }

  FastLED.show();
  delay(2000);
}
